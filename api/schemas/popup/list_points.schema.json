{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://ilearnhow.com/schemas/popup/list_points.schema.json",
  "title": "List Points Popup",
  "type": "object",
  "required": ["items", "at_ms"],
  "properties": {
    "title": { "type": ["string", "null"] },
    "items": {
      "type": "array",
      "minItems": 1,
      "items": { "type": "string", "minLength": 1 }
    },
    "at_ms": { "type": "integer", "minimum": 0 }
  }
}
